    def Convert(self):
        enteha=int(self.Enteha.toPlainText())
        ebteda=int(self.Ebteda.toPlainText())
        try:
            if enteha>ebteda:
                def is_prime(n):
                    avval=True
                    for i in range(2,int(n**0.5)+1):
                        if n%i==0:
                            avval=False
                            break
                    return(avval)

                prime_count=0
                last_prime_number=0
                
                for i in range(ebteda,enteha+1):
                    if is_prime(i):
                        last_prime_number=i
                        prime_count=prime_count+1
                        self.Javab.append(str(i))
                self.Javab.append(" از بین این اعداد شما "+str(prime_count)+" عدد اول داشتید")
                self.Javab.append('  آخرین عدد'+str(last_prime_number))



                

        except ValueError:
            self.Javab.append('خطا')

