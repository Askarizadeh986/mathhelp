    def BMI_Convert(self):
        height=self.Height.toPlainText()
        weigh=self.Weigh.toPlainText()
        BMI=float(weigh)/(float(height)/100)**2
        self.BMI_2.append(str(int(BMI)))
        if BMI<=18.5:
            self.Status.append('لاغر و کم وزن')
        elif BMI>=18.5 and BMI<=24.9:
            self.Status.append('وزن طبیعی')
        elif BMI>=25 and BMI<=29.9:
            self.Status.append('اصافه وزن')
        elif BMI <=30 and BMI >=39.9:
            self.Status.append('چاقی')
        elif BMI>=40:
            self.Status.append('چاق بسیار خطرناک')
